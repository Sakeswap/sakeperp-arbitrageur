{
    "name": "sakeperp-arbitrageur",
    "version": "0.1.0",
    "description": "",
    "scripts": {
        "codegen": "typechain --target ethers-v5 --outDir ./types/ethers ./node_modules/@sakeperp/artifact/src/**/*.json",
        "compile": "tsc -b .",
        "compile-watch": "tsc -b --watch .",
        "build": "npm run codegen && npm run compile",
        "clean": "rm -rf ./build ./.serverless ./nyc_output",
        "arbitrage": "env $(cat ./config/.env.production | grep -v '#' | xargs) ts-node --files src/index.ts",
        "invoke-local": "sls invoke local --stage production --function arbitrageur",
        "deploy": "npm run build && sls deploy --aws-s3-accelerate --stage production"
    },
    "author": "",
    "license": "BSD-3-Clause",
    "dependencies": {
        "@luminati-io/socksv5": "0.0.7",
        "@sakeperp/artifact": "1.0.4",
        "@types/nodemailer": "^6.3.0",
        "@types/fs-extra": "8.1.0",
        "async-mutex": "0.2.6",
        "big.js": "5.2.2",
        "binance-api-client": "^2.10.2",
        "ccxt": "1.49.59",
        "ethers": "5.0.19",
        "form-data": "^4.0.0",
        "ftx-api-rest": "0.0.6",
        "https-proxy-agent": "^5.0.0",
        "i": "^0.3.6",
        "lodash": ">=4.17.19",
        "log4js": "6.2.1",
        "luxon": "1.23.0",
        "node-fetch": "2.6.1",
        "reflect-metadata": "0.1.13",
        "source-map-support": "0.5.19",
        "typedi": "0.8.0",
        "crypto-js": "^4.0.0",
        "axios": "^0.21.1",
        "nodemailer": "^6.3.0",
        "fs-extra": "8.1.0"
    },
    "devDependencies": {
        "@testdeck/di-typedi": "0.0.10",
        "@typechain/ethers-v5": "3.0.0",
        "@types/big.js": "4.0.5",
        "@types/lodash": "4.14.154",
        "@types/luxon": "1.22.0",
        "@types/node": "13.13.4",
        "@types/node-fetch": "2.5.7",
        "@types/crypto-js": "^3.1.38",
        "@types/requestretry": "^1.12.6",
        "aws-sdk": "2.667.0",
        "dotenv": "8.2.0",
        "serverless": "1.69.0",
        "serverless-dotenv-plugin": "2.4.2",
        "serverless-offline": "6.1.4",
        "serverless-webpack": "5.3.2",
        "ts-node": "8.9.1",
        "tsconfig-paths": "3.9.0",
        "typechain": "4.0.0",
        "typedoc": "0.17.6",
        "typescript": "3.8.3",
        "webpack": "4.43.0"
    }
}
